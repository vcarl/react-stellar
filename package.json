{
  "name": "react-stellar",
  "description": "A react/redux API for interacting with Stellar Horizon",
  "version": "0.0.0-alpha.0",
  "license": "MIT",
  "author": "Carl Vitullo <carl.vitullo@gmail.com>",
  "repository": "vcarl/react-stellar",
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/react-stellar.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=15"
  },
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "npm run test -- --watch",
    "start": "cp ./index.js ./dist/index.js && cross-env NODE_ENV=development rollup -w -c | tsc -w -p tsconfig.base.json",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.base.json  && rollup -c && rimraf compiled && cp ./index.js ./dist/index.js",
    "format": "prettier --write 'src/**/*' 'test/**/*' 'README.md'",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001",
    "addc": "all-contributors add",
    "start-website": "cd website && yarn start",
    "build-website": "cd website && yarn install && yarn build",
    "gen-docs": "all-contributors generate && doctoc README.md",
    "size": "size-limit"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "big.js": "^5.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "create-react-context": "^0.2.2",
    "css-loader": "0.28.7",
    "deepmerge": "^2.1.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "hoist-non-react-statics": "^2.5.5",
    "html-webpack-plugin": "2.29.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.topath": "4.5.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.1",
    "react-fast-compare": "^1.0.0",
    "react-lifecycles-compat": "^3.0.4",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "stellar-sdk": "^0.9.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslib": "^1.9.3",
    "tslint-config-prettier": "^1.10.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "warning": "^3.0.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@storybook/addon-options": "^3.4.8",
    "@storybook/react": "^3.4.0",
    "@types/big.js": "^4.0.5",
    "@types/jest": "^22.2.3",
    "@types/react": "16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/react-test-renderer": "15.5.2",
    "@types/stellar-sdk": "^0.8.5",
    "@types/warning": "^3.0.0",
    "all-contributors-cli": "^4.4.0",
    "awesome-typescript-loader": "^3.4.1",
    "cross-env": "5.0.5",
    "doctoc": "^1.3.0",
    "husky": "0.14.3",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "lint-staged": "4.0.2",
    "prettier": "1.11.1",
    "raw-loader": "^0.5.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "1.4.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "size-limit": "^0.17.0",
    "ts-jest": "^22.4.6",
    "tsc-watch": "^1.0.21",
    "tslint": "5.5.0",
    "tslint-react": "3.2.0",
    "typescript": "^3.0.0-dev.20180711",
    "yup": "0.21.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/setupTest.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "14 kB"
    },
    {
      "path": "./dist/react-stellar.esm.js",
      "limit": "14 kB"
    },
    {
      "path": "./dist/react-stellar.umd.production.js",
      "limit": "14 kB"
    }
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
